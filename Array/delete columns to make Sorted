class Solution:
    def minDeletionSize(self, strs: List[str]) -> int:
        rows=len(strs)
        columns=len(strs[0])
        delete_count=0
        for col in range(columns):
            for row in range(rows-1):
                if strs[row][col]>strs[row+1][col]:
                    delete_count+=1
                    break
        return delete_count
        
